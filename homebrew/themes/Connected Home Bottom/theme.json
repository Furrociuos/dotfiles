{
  "name": "Connected Home Bottom",
  "author": "Akaza Renn",
  "manifest_version": 5,
  "version": "v1.1",
  "inject": {
    "shared.css": [
      "SP"
    ]
  },
  "patches": {
    "Expose More Cover": {
      "type": "checkbox",
      "default": "No",
      "values": {
        "Yes": {
          "--CHB-covered-area": [
            "0%",
            "SP"
          ]
        },
        "No": {
          "--CHB-covered-area": [
            "10%",
            "SP"
          ]
        }
      }
    }
  },
  "id": "83e95b73-5a31-4479-b2bc-223d284fbbf1"
}